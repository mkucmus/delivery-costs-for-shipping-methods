{
    "openapi": "3.0.0",
    "info": {},
    "paths": {
        "/shipping-methods-with-costs": {
            "get": {
                "tags": ["Store API", "Shipping"],
                "summary": "Get all available shipping methods with calculated costs",
                "description": "Returns all available shipping methods for the current cart with their calculated costs based on cart contents, context, and rules",
                "operationId": "getShippingMethodsWithCosts",
                "parameters": [
                    {
                        "name": "token",
                        "in": "query",
                        "description": "Cart token (optional, uses context token if not provided)",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of shipping methods with calculated costs",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "elements": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/ShippingMethodWithCost"
                                            }
                                        },
                                        "apiAlias": {
                                            "type": "string",
                                            "enum": ["shipping_method_with_cost_collection"]
                                        }
                                    }
                                },
                                "example": {
                                    "elements": [
                                        {
                                            "shippingMethod": {
                                                "id": "79ef7bea9b7d49fb9d32b235fb18aa18",
                                                "name": "Express",
                                                "technicalName": "shipping_express",
                                                "active": true,
                                                "deliveryTime": {
                                                    "name": "1-3 days",
                                                    "min": 1,
                                                    "max": 3,
                                                    "unit": "day"
                                                }
                                            },
                                            "calculatedPrice": {
                                                "unitPrice": 5.90,
                                                "totalPrice": 5.90,
                                                "quantity": 1,
                                                "calculatedTaxes": [],
                                                "taxRules": []
                                            },
                                            "selected": true,
                                            "available": true,
                                            "apiAlias": "shipping_method_with_cost"
                                        },
                                        {
                                            "shippingMethod": {
                                                "id": "a9d9cc502b3547f4a89eb2830c032c78",
                                                "name": "Standard",
                                                "technicalName": "shipping_standard",
                                                "active": true
                                            },
                                            "calculatedPrice": {
                                                "unitPrice": 3.90,
                                                "totalPrice": 3.90,
                                                "quantity": 1,
                                                "calculatedTaxes": [],
                                                "taxRules": []
                                            },
                                            "selected": false,
                                            "available": true,
                                            "apiAlias": "shipping_method_with_cost"
                                        }
                                    ],
                                    "apiAlias": "shipping_method_with_cost_collection"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": ["Store API", "Shipping"],
                "summary": "Get all available shipping methods with calculated costs",
                "description": "Returns all available shipping methods for the current cart with their calculated costs based on cart contents, context, and rules",
                "operationId": "postShippingMethodsWithCosts",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "token": {
                                        "type": "string",
                                        "description": "Cart token (optional, uses context token if not provided)"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "List of shipping methods with calculated costs",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "elements": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/ShippingMethodWithCost"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
